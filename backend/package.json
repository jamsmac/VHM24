{
  "name": "vendhub-backend",
  "version": "1.0.0",
  "description": "VendHub Manager Backend API",
  "author": "VendHub Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "./node_modules/.bin/nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS='--max-old-space-size=4096' jest",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=4096' jest --watch",
    "test:cov": "NODE_OPTIONS='--max-old-space-size=4096' jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:up": "docker-compose -f ../docker-compose.test.yml up -d",
    "test:e2e:down": "docker-compose -f ../docker-compose.test.yml down",
    "test:e2e:full": "npm run test:e2e:up && sleep 3 && npm run test:e2e; npm run test:e2e:down",
    "migration:generate": "typeorm migration:generate -d src/config/typeorm.config.ts",
    "migration:compile": "tsc src/database/migrations/*.ts --outDir dist/database/migrations --module commonjs --target es2021 --esModuleInterop --experimentalDecorators --emitDecoratorMetadata --skipLibCheck --resolveJsonModule --removeComments",
    "migration:run": "npm run migration:compile && typeorm migration:run -d typeorm-migrations-only.config.js",
    "migration:revert": "npm run migration:compile && typeorm migration:revert -d typeorm-migrations-only.config.js",
    "seed": "ts-node src/database/seeds/run-seed.ts",
    "create-superadmin": "ts-node src/database/scripts/create-superadmin.ts",
    "update-owner-password": "ts-node src/database/scripts/update-owner-password.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/lib-storage": "^3.980.0",
    "@aws-sdk/s3-request-presigner": "^3.980.0",
    "@bull-board/api": "^6.16.4",
    "@bull-board/express": "^6.16.4",
    "@bull-board/ui": "^6.14.2",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/platform-socket.io": "^11.1.12",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.5",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.12",
    "@sentry/node": "^10.38.0",
    "@types/speakeasy": "^2.0.10",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "cache-manager": "^7.2.8",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "csv-parser": "^3.0.0",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.6.0",
    "exceljs": "^4.4.0",
    "helmet": "^8.1.0",
    "i18next": "^25.8.0",
    "i18next-fs-backend": "^2.6.1",
    "ioredis": "^5.9.2",
    "jsqr": "^1.4.0",
    "keyv": "^5.6.0",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "nest-winston": "^1.10.2",
    "nodemailer": "^7.0.13",
    "openai": "^6.17.0",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.18.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.3",
    "speakeasy": "^2.0.0",
    "telegraf": "^4.15.0",
    "twilio": "^5.12.0",
    "typeorm": "^0.3.17",
    "ua-parser-js": "^2.0.8",
    "uuid": "^13.0.0",
    "web-push": "^3.6.7",
    "winston": "^3.18.3",
    "xml2js": "^0.6.2"
  },
  "optionalDependencies": {
    "puppeteer": "^24.36.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.12",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.9",
    "@types/nodemailer": "^7.0.9",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/pdfkit": "^0.17.4",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.0",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^11.0.0",
    "@types/web-push": "^3.6.3",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.54.0"
  },
  "overrides": {
    "glob": "9.3.5",
    "jws": "4.0.1",
    "js-yaml": "4.1.1",
    "tmp": "0.2.5",
    "lodash": "4.17.23"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.integration\\.spec\\.ts$",
      "\\.e2e-spec\\.ts$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "maxWorkers": "50%",
    "workerIdleMemoryLimit": "512MB",
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/workers/**",
      "!**/test/helpers/**",
      "!**/database/migrations/**",
      "!**/database/seeds/**",
      "!**/database/scripts/**",
      "!**/config/**",
      "!**/coverage-temp/**",
      "!**/*.spec.ts",
      "!**/*.e2e-spec.ts",
      "!**/main.ts",
      "!**/app.module.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@modules/(.*)$": "<rootDir>/modules/$1",
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@config/(.*)$": "<rootDir>/config/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(uuid)/)"
    ]
  }
}
