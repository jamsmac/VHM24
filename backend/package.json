{
  "name": "vendhub-backend",
  "version": "1.0.0",
  "description": "VendHub Manager Backend API",
  "author": "VendHub Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "typeorm migration:generate -d src/config/typeorm.config.ts",
    "migration:compile": "tsc src/database/migrations/*.ts --outDir dist/database/migrations --module commonjs --target es2021 --esModuleInterop --experimentalDecorators --emitDecoratorMetadata --skipLibCheck --resolveJsonModule --removeComments",
    "migration:run": "npm run migration:compile && typeorm migration:run -d typeorm-migrations-only.config.js",
    "migration:revert": "npm run migration:compile && typeorm migration:revert -d typeorm-migrations-only.config.js",
    "seed": "ts-node src/database/seeds/run-seed.ts",
    "create-superadmin": "ts-node src/database/scripts/create-superadmin.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.932.0",
    "@aws-sdk/lib-storage": "^3.932.0",
    "@aws-sdk/s3-request-presigner": "^3.936.0",
    "@bull-board/api": "^6.14.2",
    "@bull-board/express": "^6.14.2",
    "@bull-board/ui": "^6.14.2",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/terminus": "^10.3.0",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.4.20",
    "@sentry/node": "^8.55.0",
    "@types/speakeasy": "^2.0.10",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "cache-manager": "^5.7.6",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "csv-parser": "^3.0.0",
    "date-fns": "^3.0.6",
    "decimal.js": "^10.6.0",
    "exceljs": "^4.4.0",
    "helmet": "^8.1.0",
    "i18next": "^25.6.3",
    "i18next-fs-backend": "^2.6.1",
    "ioredis": "^5.8.2",
    "jsqr": "^1.4.0",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "nest-winston": "^1.10.2",
    "nodemailer": "^7.0.11",
    "openai": "^6.10.0",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "telegraf": "^4.15.0",
    "typeorm": "^0.3.17",
    "ua-parser-js": "^2.0.6",
    "uuid": "^11.1.0",
    "web-push": "^3.6.7",
    "winston": "^3.18.3",
    "xml2js": "^0.6.2"
  },
  "optionalDependencies": {
    "puppeteer": "^24.31.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^7.0.4",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/pdfkit": "^0.13.4",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.0",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^10.0.0",
    "@types/web-push": "^3.6.3",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "glob": "9.3.5",
    "jws": "4.0.1",
    "js-yaml": "4.1.1",
    "tmp": "0.2.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/workers/**",
      "!**/test/helpers/**",
      "!**/database/migrations/**",
      "!**/database/seeds/**",
      "!**/database/scripts/**",
      "!**/config/**",
      "!**/coverage-temp/**",
      "!**/*.spec.ts",
      "!**/*.e2e-spec.ts",
      "!**/main.ts",
      "!**/app.module.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@modules/(.*)$": "<rootDir>/modules/$1",
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@config/(.*)$": "<rootDir>/config/$1"
    }
  }
}
